                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        PURINVMAIN.SUPNAME, PURINVMAIN.INVNO, PURINVMAIN.INVDATE, PURINVMAIN.DUEDAYS, PURINVMAIN.SPDISCOUNT, PURINVMAIN.ADJVALUE, 
                         PURINVMAIN.WHID, PURINVMAIN.NETVALUE, ITEMMAST.ITEMNAME, ITEMMAST.BARCODE, PURINVDETAIL.QTY, PURINVDETAIL.BONUSQTY, 
                         PURINVDETAIL.INVRATE, PURINVDETAIL.DISC, PURINVDETAIL.SPDISC, PURINVDETAIL.EXPDATE, PURINVDETAIL.LINETOTAL, PURINVDETAIL.SPRICE, 
                         PURINVDETAIL.CPRICE, PURINVDETAIL.ITEMNO, CASE WHEN PURINVDETAIL.CPRICE &gt; 0 THEN ((PURINVDETAIL.SPRICE - PURINVDETAIL.CPRICE) 
                         / PURINVDETAIL.CPRICE) * 100 WHEN isnull(PURINVDETAIL.CPRICE, 0) = 0 THEN 100 END AS Profit, PURINVMAIN.HOLD
